<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ScaleView">
        <attr name="selected_color" format="color" />
        <attr name="normal_color" format="color" />
        <attr name="selected_height" format="dimension" />
        <attr name="normal_height" format="dimension" />
    </declare-styleable>
    <declare-styleable name="TextRotationView">
        <attr name="text_color" format="color"/>
        <attr name="text_size" format="dimension"/>
        <attr name="text_content" format="string"/>
        <attr name="text_rotation" format="integer"/>
    </declare-styleable>
</resources>